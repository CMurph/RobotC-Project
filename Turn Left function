void turn_left()

{
	//Make a 90 Degrees Left turn
		motor[motorA] = 25;
  	motor[motorB] = -25;
		//wait1Msec(750);
		wait1Msec(745);

	//Stop After turn left executed
		motor[motorA] = 0;
  	motor[motorB] = 0;
		wait1Msec(200);
}
